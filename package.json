{
  "name": "vimeoplaylist",
  "version": "1.1.0",
  "description": "Create endless video playlists with the Vimeo Player API.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenscaff/vimeo-playlists.git"
  },
  "bugs": {
    "url": "https://github.com/stephenscaff/read-smore/issues"
  },
  "keywords": [
    "vimeo",
    "vimeo api",
    "vimeo playlist",
    "video playlist",
    "playlist",
    "vimeo player",
    "playlists"
  ],
  "author": "Stephen Scaff <stephenscaff.com>",
  "license": "MIT",
  "homepage": "https://github.com/stephenscaff/vimeo-playlists.git#readme",
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "targets": {
    "main": false,
    "module": false,
    "browser": false
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "microbundle",
    "dev": "npm run demo:start && microbundle watch",
    "demo:start": "parcel ./demo/src/index.html --dist-dir ./demo/dist",
    "demo:build": "parcel build ./demo/src/index.html --dist-dir ./demo/dist --public-url ./",
    "demo:deploy": "npm run demo:build && gh-pages -d ./demo/dist",
    "demo:clean": "rm -rf ./demo/dist",
    "lint": "eslint ./src/**js --fix"
  },
  "dependencies": {
    "@vimeo/player": "^2.16.4"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.3.2",
    "eslint": "^8.11.0",
    "gh-pages": "^3.2.3",
    "microbundle": "^0.14.2",
    "parcel": "^2.3.2"
  }
}
